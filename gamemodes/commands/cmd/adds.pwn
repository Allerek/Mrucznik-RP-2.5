CMD:adds(playerid)
{
	if(IsPlayerConnected(playerid))
	{
		if(PlayerInfo[playerid][pLevel] >= 3)
		{
			sendTipMessage(playerid, "W trakcie prac"); 
		}
		else
		{
			sendErrorMessage(playerid, "Dodatki s¹ dostêpne od 3 lvl'a"); 
		}
	}
	return 1;
}
