YCMD:unfrakcja(playerid, params[], help)
{
    if(IsPlayerConnected(playerid))
    {
		new para1;
		if( sscanf(params, "k<fix>", para1))
		{
			sendTipMessage(playerid, "U¿yj /unfrakcja [ID gracza]");
			return 1;
		}

		UnFrakcja(playerid, para1);
	}
	return 1;
}
